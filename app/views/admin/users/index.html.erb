<% provide :title, t("Admin.All_user") %>
<h1><%= t "Admin.All_user" %></h1>
<ul class = "users">
  <% @users.each do |user| %>
    <li>
      <% if user.supervisor? %>
        <%= link_to user.name, [:admin, user] %>
      <% else %>
        <%= link_to user.name, user_path(user) %>
      <% end %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
